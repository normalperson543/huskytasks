{
  "expo": {
    "name": "Huskytasks",
    "slug": "huskytasks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trevonnguyen.huskytasks"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trevonnguyen.huskytasks"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c8053953-2af8-418d-817c-7c33e8ed6111"
      }
    },
    "owner": "trevonnguyen",
    "updates": {
      "url": "https://u.expo.dev/c8053953-2af8-418d-817c-7c33e8ed6111"
    },
    "runtimeVersion": "1.0.0"
  }
}
